{
  "name": "web-developer-mcp",
  "description": "A Model Context Protocol (MCP) server that provides web development tools for AI assistants. Enables browser automation, DOM inspection, network monitoring, and console analysis through Playwright.",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "web-development",
    "browser-automation",
    "playwright",
    "ai-assistant",
    "claude",
    "cursor",
    "dom-inspection",
    "network-monitoring",
    "console-logs",
    "debugging",
    "testing",
    "web-scraping"
  ],
  "author": "Christoffer Artmann <artgaard@gmail.com>",
  "version": "1.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Artmann/web-developer-mcp.git"
  },
  "homepage": "https://github.com/Artmann/web-developer-mcp",
  "bugs": {
    "url": "https://github.com/Artmann/web-developer-mcp/issues"
  },
  "main": "dist/index.js",
  "module": "src/index.ts",
  "type": "module",
  "bin": {
    "web-developer-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier --write .",
    "prepublishOnly": "bun run build",
    "start": "bun src/index.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "playwright": "^1.55.1",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.18.6",
    "@vitest/ui": "^2.1.9",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "vitest": "^3.2.4"
  }
}
